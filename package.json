{
  "name": "multi-avatar",
  "version": "0.1.1",
  "description": "Package to fetch user avatars from various social-sites",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib/",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/mocha --require=any-promise/register/bluebird --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maksimkurb/multi-avatar.git"
  },
