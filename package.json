    "twitter",
    "g+"
  ],
  "author": "Maxim Kurbatov <maksimkurb@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "any-promise": "^1.3.0",
    "node-fetch": "^1.5.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "bluebird": "^3.4.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "mocha": "^2.5.3"
  }
